<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <item
        android:width="@dimen/margin_small"
        android:height="@dimen/margin_small"
        android:end="@dimen/margin_small"
        android:start="@dimen/margin_small">
        <shape android:shape="rectangle">
            <solid android:color="@color/light_gray" />
            <corners android:radius="@dimen/margin_large" />
            <size
                android:width="@dimen/standard_margin"
                android:height="@dimen/standard_margin" />
        </shape>

    </item>

</layer-list>