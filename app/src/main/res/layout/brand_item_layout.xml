<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="data"
            type="com.example.bunonbasket.data.models.partners.PartnerModel" />
    </data>


    <androidx.cardview.widget.CardView
        android:layout_width="@dimen/size_133"
        android:layout_height="@dimen/size_85"
        app:cardElevation="@dimen/margin_2"
        app:cardUseCompatPadding="true"
        app:cardCornerRadius="@dimen/margin_small">

        <ImageView
            android:layout_width="@dimen/size_64"
            android:layout_height="@dimen/margin_large"
            android:scaleType="fitXY"
            android:layout_gravity="center"
            app:patchImageFullUrl="@{data.fullImageUrl}" />

    </androidx.cardview.widget.CardView>

</layout>